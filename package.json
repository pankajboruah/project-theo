{
	"name": "scaffold",
	"version": "0.1.0",
	"private": true,
	"engines": {
		"node": ">=14",
		"npm": ">=6"
	},
	"scripts": {
		"start": "concurrently \"craco start\" \"npm run lint\"",
		"build": "craco build",
		"test": "craco test",
		"eject": "react-scripts eject",
		"server": "json-server --watch server.js --port 4001",
		"eslint": "eslint --ignore-path .eslintignore ./src",
		"styleLint": "stylelint \"**/*.scss\" --config .stylelintrc",
		"lint": "concurrently \"npm run eslint\" \"npm run styleLint\"",
		"lint:fix": "concurrently \"npm run eslint --fix\" \"npm run styleLint --fix\""
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"eslintIgnore": [
		"*.test.js",
		"*.spec.js"
	],
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@craco/craco": "^6.4.3",
		"@testing-library/jest-dom": "^5.16.1",
		"@testing-library/react": "^12.1.2",
		"@testing-library/user-event": "^13.5.0",
		"antd": "^4.18.2",
		"axios": "^0.24.0",
		"classnames": "^2.3.1",
		"json-server": "^0.17.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-scripts": "5.0.0",
		"web-vitals": "^2.1.2"
	},
	"devDependencies": {
		"@babel/core": "^7.16.7",
		"@babel/eslint-parser": "^7.16.5",
		"@babel/plugin-transform-react-jsx": "^7.16.7",
		"@babel/preset-react": "^7.16.7",
		"babel-loader": "^8.2.3",
		"babel-plugin-react-css-modules": "^5.2.6",
		"concurrently": "^6.5.1",
		"craco-alias": "^3.0.1",
		"craco-image-optimizer-plugin": "0.0.5",
		"craco-sass-resources-loader": "^1.1.0",
		"craco-workbox": "^0.2.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^8.3.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.28.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"node-sass": "^7.0.1",
		"prettier": "^2.5.1",
		"sass-loader": "^12.4.0",
		"stylelint": "^14.2.0",
		"stylelint-config-recommended": "^6.0.0",
		"stylelint-config-standard": "^24.0.0",
		"stylelint-config-styled-components": "^0.1.1",
		"stylelint-processor-styled-components": "^1.10.0",
		"url-loader": "^4.1.1"
	}
}
